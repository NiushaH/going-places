<h1>Sign Up for Going Places</h1>

<%= form_tag("/users") do %>
<p>
  <%= label_tag "name", "Name: " %>
  <%= text_field_tag "name" %>
</p>
<p>
  <%= label_tag "email", "Email: " %>
  <%= text_field_tag "email" %>
</p>
<p>
  <%= label_tag "password", "Password: " %>
  <%= password_field_tag "password" %>
</p>

  <%= submit_tag "Sign Up" %>
<% end %>